{
  "DefaultConnection": "Database connection string",
  "Environment": "Dev",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AWS.Logging": {
    "IncludeCategory": false,
    "IncludeLogLevel": true,
    "IncludeNewline": true,
    "IncludeContextHeaders": [
      "x-request-id",
      "x-api-userid"
    ],
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Error",
      "Microsoft.EntityFrameworkCore": "Error",
      "Microsoft.AspNetCore.Mvc": "Error",
      "Microsoft.AspNetCore.Mvc.Internal.ControllerActionInvoker": "Error",
      "Microsoft.AspNetCore.Hosting.Internal.WebHost": "Error",
      "System": "Error"
    }
  },
  "SwaggerSettings": {
    "Stage": "local",
    "JsonUrl": "swagger.json",
    "IncludeXApiKeyAuthorization": true,
    "Decription": "Customer API",
    "SwaggerDocName": "v1",
    "Info": {
      "Title": "",
      "Description": ""
    },
    "XmlCommentsFile": "Customer.Api.xml",
    "AuthorizeHeaders": [
      {
        "Name": "API Key",
        "Header": "x-api-key"
      },
      {
        "Name": "XRequestId Key",
        "Header": "x-request-id"
      },
      {
        "Name": "XpiClient Key",
        "Header": "x-api-client"
      }
    ]
  },
  "AllowedHosts": "*"
}